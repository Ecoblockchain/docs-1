{
	"name": "tdt/docs",
	"description": "The datatank documentation.",
	"keywords": ["docs", "datatank", "datatank documentation"],
	"license": "aGPLv3",
	"require": {
		"laravel/framework": "4.0.*",
		"michelf/php-markdown": "1.3.*@dev",
		"twitter/bootstrap":"*"
	},
	"autoload": {
		"classmap": [
			"app/controllers"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
